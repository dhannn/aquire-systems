{{!-- Grade List Partial for Admin_Student --}}

{{#if current}}
{{!-- GRADE LEVEL ACTIVE ON PAGE --}}
    <li class="nav-item">
        <a class="nav-link active fs-5" aria-current="page" href="/admin/students?grade={{label}}"><u>{{label}}</u></a>
    </li>

{{!-- GRADE LEVEL NOT ACTIVE ON PAGE --}}
    {{else}}
        <li class="nav-item">
            <a class="nav-link fs-5" href="/admin/students?grade={{label}}">{{label}}</a>
        </li>
{{/if}}